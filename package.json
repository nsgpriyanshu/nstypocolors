{
  "name": "nstypocolors",
  "version": "1.2.11",
  "description": "A typescript color package specially for typescript developers.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "format": "npx prettier . --write",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "cleanStart": "npm run clean && npm run build && npm run start",
    "noCleanStart": "npm run build && npm run start",
    "execute": "npm run clean && npm run build && npm run format && npm run start",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix"
  },
  "keywords": [
    "nstypocolors",
    "nstc",
    "color"
  ],
  "author": "nsgpriyanshu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nsgpriyanshu/nstypocolors"
  },
  "files": [
    "dist/",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
