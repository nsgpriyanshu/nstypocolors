{
  "name": "nstypocolors",
  "version": "1.4.0",
  "description": "A typescript color package specially for typescript developers.",
  "main": "dist/src/index.js",
  "types": "types/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist types",
    "format:check": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "cleanStart": "npm run clean && npm run build && npm run start",
    "noCleanStart": "npm run build && npm run start",
    "execute": "npm run clean && npm run build && npm run format && npm run start",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "release": "cliff-jumper"
  },
  "keywords": [
    "nstypocolors",
    "nstc",
    "typescript colors",
    "typocolors",
    "colors"
  ],
  "author": "nsgpriyanshu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nsgpriyanshu/nstypocolors"
  },
  "bugs": {
    "url": "https://github.com/nsgpriyanshu/nstypocolors/issues"
  },
  "homepage": "https://nsgpriyanshu.github.io/nstypocolors",
  "files": [
    "dist/",
    "types/",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-angular": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@favware/cliff-jumper": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
